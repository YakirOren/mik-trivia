        mc:Ignorable="d"
        Title="stats" Height="450" Width="800">

    <Grid RenderTransformOrigin="0.678,0.505" Style="{DynamicResource app_backgroud}">


        <Border BorderBrush="White" BorderThickness="2.5" CornerRadius="8">
